{
  "name": "redux-flux-store",
  "version": "0.2.0",
  "description": "Redux store enhancer which converts Flux like stores into a reducer function",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "clean": "rm -rf lib",
    "watch": "babel -d lib/ src/ --watch --sourceMaps=true",
    "test-watch": "onchange './src/**/*.js' -- tape ./lib/**/__specs__/*.js",
    "prepublish": "npm run clean; npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/samiskin/redux-flux-store.git"
  },
  "keywords": [
    "redux",
    "reducer",
    "flux",
    "store",
    "traditional",
    "state"
  ],
  "author": "Shiranka Miskin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samiskin/redux-flux-store/issues"
  },
  "homepage": "https://github.com/samiskin/redux-flux-store#readme",
  "dependencies": {
    "lodash": "^3.10.1",
    "object-merge": "^2.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.17",
    "babel-eslint": "^4.1.6",
    "babel-preset-es2015": "^6.3.13",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "onchange": "^2.0.0",
    "redux": "^3.0.5",
    "tape": "^4.2.2"
  }
}
